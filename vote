print('Escolha um: XBOX / PLAYSTATION / NINTENDO')
voto1 = input('Digite sua escolha: ')
voto2 = input('Digite sua escolha: ')
voto3 = input('Digite sua escolha: ')
voto4 = input('Digite sua escolha: ')
voto5 = input('Digite sua escolha: ')
xbox = 0
playstation = 0
nintendo = 0

if voto1.upper() == 'XBOX':
    xbox = xbox + 1
elif voto1.upper() == 'PLAYSTATION':
    playstation = playstation + 1
elif voto1. upper() == 'NINTENDO':
    nintendo = nintendo + 1
else:
    print('O membro digitou uma opção invalída')

if voto2.upper() == 'XBOX':
    xbox = xbox + 1
elif voto2.upper() == 'PLAYSTATION':
    playstation = playstation + 1
elif voto2. upper() == 'NINTENDO':
    nintendo = nintendo + 1
else:
    print('O membro digitou uma opção invalída')

if voto3.upper() == 'XBOX':
    xbox = xbox + 1
elif voto3.upper() == 'PLAYSTATION':
    playstation = playstation + 1
elif voto3. upper() == 'NINTENDO':
    nintendo = nintendo + 1
else:
    print('O membro digitou uma opção invalída')

if voto4.upper() == 'XBOX':
    xbox = xbox + 1
elif voto4.upper() == 'PLAYSTATION':
    playstation = playstation + 1
elif voto4. upper() == 'NINTENDO':
    nintendo = nintendo + 1
else:
    print('O membro digitou uma opção invalída')

if voto5.upper() == 'XBOX':
    xbox = xbox + 1
elif voto5.upper() == 'PLAYSTATION':
    playstation = playstation + 1
elif voto5. upper() == 'NINTENDO':
    nintendo = nintendo + 1
else:
    print('O membro digitou uma opção invalída')

if xbox > playstation and xbox > nintendo:
    print('O console escolhido foi o Xbox')
elif playstation > xbox and playstation > nintendo:
    print('O console escolhido foi o Playstation')
elif nintendo > xbox and nintendo > playstation:
    print('O console escolhido foi o Nintendo')
else:
    print('Houve um empate, reinicie a votação')
